<p id="notice"><%= notice %></p>

<h1>Esparams</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Tipoestado</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @esparams.each do |esparam| %>
      <tr>
        <td><%= esparam.name %></td>
        <td><%= esparam.tipoestado %></td>
        <td><%= link_to 'Exibir', esparam %></td>
        <td><%= link_to 'Editar', edit_esparam_path(esparam) %></td>
        <td><%= link_to 'Remover', esparam, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Esparam', new_esparam_path %>
