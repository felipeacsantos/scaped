<p id="notice"><%= notice %></p>

<h2>Parametrizações</h2>

<table>
  <thead>
    <tr>
      <th>Objeto</th>
      <th>Parametro</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @parametrizacaos.each do |parametrizacao| %>
      <tr>
        <td><%= parametrizacao.objeto %></td>
        <td><%= parametrizacao.parametro %></td>
        <td><%= link_to 'Exibir', parametrizacao %></td>
        <td><%= link_to 'Editar', edit_parametrizacao_path(parametrizacao) %></td>
        <td><%= link_to 'Remover', parametrizacao, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'Nova Parametrização', new_parametrizacao_path %>
