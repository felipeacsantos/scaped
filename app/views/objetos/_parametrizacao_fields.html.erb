<li class="control-group nested-fields">
  <div class="controls">
    <%= f.fields_for :parametro do |parametrizacao_parametro| %>
      <%= parametrizacao_parametro.text_field :name %>
      <%= parametrizacao_parametro.text_field :value %>
    <% end %>

    <%= link_to_remove_association "remove", f %>
  </div>
</li>
