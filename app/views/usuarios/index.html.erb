<p id="notice"><%= notice %></p>

<h1>Usuarios</h1>

<table>
  <thead>
    <tr>
      <th>Primeironome</th>
      <th>Sobrenome</th>
      <th>Email</th>
      <th>Senha</th>
      <th>Datanasc</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @usuarios.each do |usuario| %>
      <tr>
        <td><%= usuario.primeironome %></td>
        <td><%= usuario.sobrenome %></td>
        <td><%= usuario.email %></td>
        <td><%= usuario.senha %></td>
        <td><%= usuario.datanasc %></td>
        <td><%= link_to 'Show', usuario %></td>
        <td><%= link_to 'Edit', edit_usuario_path(usuario) %></td>
        <td><%= link_to 'Destroy', usuario, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Usuario', new_usuario_path %>
