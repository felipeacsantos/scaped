<%= form_for(parametro) do |f| %>
  <% if parametro.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(parametro.errors.count, "error") %> prohibited this parametro from being saved:</h2>

      <ul>
      <% parametro.errors.full_messages.each do |message| %>
        <li><%= message %></li>
      <% end %>
      </ul>
    </div>
  <% end %>

  <div class="field">
    <%= f.label :name %>
    <%= f.text_field :name %>
  </div>

  <div class="field">
    <%= f.label :tipoobjeto %>
    <%= f.collection_select :tipoobjeto, Tipoobjeto.all, :id, :descricao %>
  </div>

  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>
