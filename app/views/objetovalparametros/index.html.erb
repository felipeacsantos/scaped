<p id="notice"><%= notice %></p>

<h2>Objeto-Parâmetros</h2>

<table>
  <thead>
    <tr>
      <th>Objeto</th>
      <th>Valparametro</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @objetovalparametros.each do |objetovalparametro| %>
      <tr>
        <td><%= objetovalparametro.objeto_id %></td>
        <td><%= objetovalparametro.valparametro_id %></td>
        <td><%= link_to 'Exibir', objetovalparametro %></td>
        <td><%= link_to 'Editar', edit_objetovalparametro_path(objetovalparametro) %></td>
        <td><%= link_to 'Remover', objetovalparametro, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'Novo Objeto-Parâmetro', new_objetovalparametro_path %>
